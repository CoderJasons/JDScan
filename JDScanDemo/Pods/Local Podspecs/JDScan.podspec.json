{
  "name": "JDScan",
  "version": "1.0",
  "summary": "ios opencv zxing scan wrapper",
  "homepage": "https://github.com/wangjindong",
  "license": "MIT",
  "authors": {
    "WJD": "419591321@qq.com"
  },
  "source": {
    "git": "https://github.com/wangjindong/JDScan.git",
    "tag": "1.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": "All",
  "subspecs": [
    {
      "name": "ZXing",
      "source_files": "JDScan/ZXing/**/*.{h,m,mm}",
      "vendored_frameworks": "JDScan/Frameworks/*.framework",
      "libraries": [
        "c++",
        "c",
        "c++abi"
      ],
      "dependencies": {
        "ZXingObjC": [
          "3.1.1"
        ]
      }
    },
    {
      "name": "UI",
      "source_files": "JDScan/UI/*.{h,m}",
      "resources": "JDScan/UI/CodeScan.bundle"
    },
    {
      "name": "All",
      "dependencies": {
        "JDScan/ZXing": [

        ],
        "JDScan/UI": [

        ]
      }
    }
  ]
}
